{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "includeFiles": ["api/**"]
    }
  },
  "ignore": ["server/**"],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}